<div class="under-construction">
    <h1>Under-construction</h1>
  </div>
<div class="container-fluid">

  <header role="banner">
    {{ page.header }}
  </header>

  <div class="row">
    {{ page.primary_menu }}
  </div>

  <div class="row">
    <div class="col-md-12">
       <div class="blog-view">
           {{ page.content }}
       </div>
    </div>
  </div>

 

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}

</div>{# container #}

